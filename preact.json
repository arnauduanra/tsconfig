{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 12",
  "compilerOptions": {
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node", 
      "esModuleInterop": true,
      "module": "ESNext", 
      "lib": ["ES2015"],
      "target": "ES5",  
      "jsx": "preserve",
      "jsxFactory": "h",
      "jsxFragmentFactory": "Fragment"
  }
}
